{
  "name": "@brueshi/linear-cli",
  "version": "0.0.10",
  "description": "Command line interface for Linear issue management. Create issues, manage workflows, and generate branches without leaving the terminal.",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "linear": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build",
    "bun:dev": "bun run src/index.ts",
    "bun:build": "bun build src/index.ts --outdir dist --target bun",
    "bun:start": "bun dist/index.js",
    "bun:compile": "bun build src/index.ts --compile --outfile linear-bun"
  },
  "keywords": [
    "linear",
    "cli",
    "issue-tracker",
    "project-management",
    "developer-tools",
    "productivity",
    "ai",
    "claude",
    "natural-language",
    "bun"
  ],
  "author": "joe bruechner",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brueshi/linear-cli.git"
  },
  "homepage": "https://github.com/brueshi/linear-cli#readme",
  "bugs": {
    "url": "https://github.com/brueshi/linear-cli/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CLAUDE.md",
    "AGENTS.md"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@inquirer/prompts": "^8.0.1",
    "@linear/sdk": "^65.0.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/bun": "^1.2.14",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
